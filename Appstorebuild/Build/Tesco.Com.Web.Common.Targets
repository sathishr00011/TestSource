<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\Microsoft\VisualStudio\TeamBuild\Microsoft.TeamFoundation.Build.targets" />
  <PropertyGroup>
    <TfCommand>"C:\Program Files (x86)\Microsoft Visual Studio 10.0\Common7\IDE\tf.exe"</TfCommand>
    <WebCore>Tesco.Com.Web.Core.dll</WebCore>
    <WebResources>Tesco.Com.Web.Resources.dll</WebResources>
    <WebViewModels>Tesco.Com.Web.ViewModels.dll</WebViewModels>
    <WebTesting>Tesco.Com.Web.Testing.dll</WebTesting>
  </PropertyGroup>

  <Target Name="MyTarget">
    <!--Change the foo below with the name of your file name propert property  created above-->

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(WebCore)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(WebCore)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(WebCore)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(WebResources)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(WebResources)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(WebResources)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(WebViewModels)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(WebViewModels)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(WebViewModels)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(WebTesting)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(WebTesting)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(WebTesting)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

  </Target>

</Project>
