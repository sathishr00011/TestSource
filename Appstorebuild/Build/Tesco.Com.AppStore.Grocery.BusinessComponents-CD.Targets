<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\Microsoft\VisualStudio\TeamBuild\Microsoft.TeamFoundation.Build.targets" />
  <PropertyGroup>
    <TfCommand>"C:\Program Files (x86)\Microsoft Visual Studio 10.0\Common7\IDE\tf.exe"</TfCommand>
    <AppStoreGroceryBusinessContractsAccount>Tesco.Com.AppStore.Grocery.Business.Account.Contracts.dll</AppStoreGroceryBusinessContractsAccount>
    <AppStoreGroceryBusinessContractsCheckout>Tesco.Com.AppStore.Grocery.Business.Checkout.Contracts.dll</AppStoreGroceryBusinessContractsCheckout>
    <AppStoreGroceryBusinessContractsCustomer>Tesco.Com.AppStore.Grocery.Business.Customer.Contracts.dll</AppStoreGroceryBusinessContractsCustomer>
    <AppStoreGroceryBusinessContractsShopping>Tesco.Com.AppStore.Grocery.Business.Shopping.Contracts.dll</AppStoreGroceryBusinessContractsShopping>
    <AppStoreGroceryBusinessContractsProduct>Tesco.Com.AppStore.Grocery.Business.Product.Contracts.dll</AppStoreGroceryBusinessContractsProduct>
    <AppStoreGroceryBusinessContractsContent>Tesco.Com.AppStore.Grocery.Business.Content.Contracts.dll</AppStoreGroceryBusinessContractsContent>
    <AppStoreGroceryBusinessContractsPayment>Tesco.Com.AppStore.Grocery.Business.Payment.Contracts.dll</AppStoreGroceryBusinessContractsPayment>
  </PropertyGroup>
  
  <Target Name="MyTarget">
     <!--Business Contracts-->
    
    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsAccount)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(AppStoreGroceryBusinessContractsAccount)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsAccount)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsCheckout)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(AppStoreGroceryBusinessContractsCheckout)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsCheckout)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsCustomer)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(AppStoreGroceryBusinessContractsCustomer)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsCustomer)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsShopping)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(AppStoreGroceryBusinessContractsShopping)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsShopping)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

<Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsProduct)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(AppStoreGroceryBusinessContractsProduct)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsProduct)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsContent)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(AppStoreGroceryBusinessContractsContent)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsContent)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsPayment)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(AppStoreGroceryBusinessContractsPayment)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(AppStoreGroceryBusinessContractsPayment)" ContinueOnError="true" />
    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />
  </Target>
</Project>
