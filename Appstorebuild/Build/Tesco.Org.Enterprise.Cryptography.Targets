<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\Microsoft\VisualStudio\TeamBuild\Microsoft.TeamFoundation.Build.targets" />

  <PropertyGroup>
   <TfCommand>"C:\Program Files (x86)\Microsoft Visual Studio 10.0\Common7\IDE\tf.exe"</TfCommand>
	<Platform>Tesco.Org.Enterprise.Cryptography.Platform.dll</Platform>
	<Platform_RSA>Tesco.Org.Enterprise.Cryptography.Provider.dll</Platform_RSA>
	<Platform_Config>Tesco.Org.Enterprise.Cryptography.Configuration.dll</Platform_Config>
	<Token>Tesco.Org.Enterprise.Cryptography.Tokenisation.dll</Token>
  </PropertyGroup>
  <Target Name="MyTarget">
	
    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(Platform)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(Platform)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(Platform)" ContinueOnError="false" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(Platform_RSA)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(Platform_RSA)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(Platform_RSA)" ContinueOnError="false" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(Platform_Config)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(Platform_Config)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(Platform_Config)" ContinueOnError="false" />

    <Exec Command="$(TfCommand) checkout /lock:none $(MSBuildProjectDirectory)\..\Lib\Tesco\$(Token)" ContinueOnError="true" />
    <Copy SourceFiles="$(MSBuildProjectDirectory)\..\..\..\..\..\Binaries\$(Token)" DestinationFiles="$(MSBuildProjectDirectory)\..\Lib\Tesco\$(Token)" ContinueOnError="false" />


    <Exec Command="$(TfCommand) checkin /bypass /noprompt /override:'***TARGET_CHECKIN***' /Comment:'***NO_CI***'" ContinueOnError="true" />
  
  </Target>
</Project>
