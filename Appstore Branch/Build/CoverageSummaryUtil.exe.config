<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="BuildDefs">
      <section name="GroceryHost" type="System.Configuration.DictionarySectionHandler"/>
      <section name="CustomerProfile" type="System.Configuration.DictionarySectionHandler"/>
      <section name="Home" type="System.Configuration.DictionarySectionHandler"/>
      <section name="BasketBuilding" type="System.Configuration.DictionarySectionHandler"/>
      <section name="FindProducts" type="System.Configuration.DictionarySectionHandler"/>
      <section name="Delivery" type="System.Configuration.DictionarySectionHandler"/>
      <section name="OrderCheckout" type="System.Configuration.DictionarySectionHandler"/>
         
    </sectionGroup>
  </configSections>

  <appSettings>
    <add key="Uri" value="http://tfsapp.dotcom.tesco.org/tfs/grocery" />
    <add key="SMTPServer" value="172.25.58.118" />
    <!--<add key="SMTPServer" value="UKDBT66DEMOS01V" />-->
    <add key="FromAddress" value="IGHS_Community_DotNet@in.tesco.com" />
    <add key="Project" value="TescoAppStore" />
    <add key="EmailTo" value="IGHS_Scrum_of_Scrum@uk.tesco.com,IGHS_Community_TechLeads@in.tesco.com,IGHS_Community_DotNet@in.tesco.com,IGHS_Team_Jaguars@in.tesco.com,HSC_IGHS_Team_Vikings@in.tesco.com" />
    <add key ="MailSubject" value="Grocerycomponents Unit Test Code Coverage Report" />
  </appSettings>

  <BuildDefs>
    <GroceryHost>
      <add key="TescoComWebUIGroceryHost" value="Tesco.Com.Web.UI.GroceryHost" />
      <add key="TescoComWebUIGroceryCommonResource" value="Tesco.Com.Web.UI.Grocery.Common.Resource" />
    </GroceryHost>

    <CustomerProfile>
      <add key="TescoComWebUIGroceryMyAccount" value="Tesco.Com.Web.UI.Grocery.MyAccount" />
      <add key="AppStoreLocationContracts" value="AppStore.Location.Contracts" />
      <add key="AppStoreLocationWcfClient" value="AppStore.Location.WcfClient" />
      <add key="AppStoreLoyaltyContracts" value="AppStore.Loyalty.Contracts" />
      <add key="AppStoreLoyaltyWcfClient" value="AppStore.Loyalty.WcfClient" />
      <add key="AppStoreGroceryBusinessCustomer" value="AppStore.Grocery.Business.Customer" />
     </CustomerProfile>

    <Home>
      <add key="TescoComWebUIGroceryHome" value="Tesco.Com.Web.UI.Grocery.Home" />
      <add key="DigitalContentClient_build" value="DigitalContentClient" />
      <add key="AppStoreGroceryBusinessContent" value="AppStore.Grocery.Business.Content" />
    </Home>

    <BasketBuilding>
      <add key="TescoComWebUIGroceryShoppingCart" value="Tesco.Com.Web.UI.Grocery.ShoppingCart" />
      <add key="AppStoreGroceryBusinessShopping" value="AppStore.Grocery.Business.Shopping" />
      <add key="AppStoreShoppingCartContracts" value="AppStore.ShoppingCart.Contracts" />
    </BasketBuilding>

    <FindProducts>
      <add key="TescoComWebUIGroceryProducts" value="Tesco.Com.Web.UI.Grocery.Products" />
      <add key="AppstoreProductAdapter" value="Appstore.ProductAdapter" />
      <add key="TescoComWebAPIClientProductListClient" value="Tesco.Com.WebAPIClient.ProductListClient" />
      <add key="AppStoreGroceryBusinessProduct" value="AppStore.Grocery.Business.Product" />
      <add key="AppStoreFavouriteContracts" value="AppStore.Favourite.Contracts" />
      <add key="AppStoreFavouriteWcfClient" value="AppStore.Favourite.WcfClient" />
      <add key="AppStoreGroceryProductWraper" value="AppStore.GroceryProduct.Wraper" />
      <add key="AppStoreProductWraper" value="AppStore.Product.Wraper" />
    </FindProducts>


    <Delivery>
      <add key="TescoComWebUIGroceryDelivery" value="Tesco.Com.Web.UI.Grocery.Delivery" />
      <add key="AppStoreDeliveryContracts" value="AppStore.Delivery.Contracts" />
      <add key="AppstoreDeliveryAdapter" value="Appstore.DeliveryAdapter" />
      <add key="TescoTransportAPIClient" value="Tesco.TransportAPIClient" />
      <add key="AppStoreGroceryBusinessDelivery" value="AppStore.Grocery.Business.Delivery" />
     </Delivery>

    <OrderCheckout>
      <add key="TescoComWebUIGroceryCheckout" value="Tesco.Com.Web.UI.Grocery.Checkout" />
      <add key="AppStoreOrderContracts" value="AppStore.Order.Contracts" />
      <add key="AppStoreOrderWcfClient" value="AppStore.Order.WcfClient" />
      <add key="AppStorePaymentContracts" value="AppStore.Payment.Contracts" />
      <add key="AppStorePaymentWcfClient" value="AppStore.Payment.WcfClient" />
      <add key="AppStoreCouponContracts" value="AppStore.Coupon.Contracts" />
      <add key="AppStoreCouponWcfClient" value="AppStore.Coupon.WcfClient" />
      <add key="AppStoreGroceryBusinessOrderCheckout" value="AppStore.Grocery.Business.OrderCheckout" />
    </OrderCheckout>





  </BuildDefs>

</configuration>